<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="439.0" prefWidth="584.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="cn.yansui.module.main.controller.MainController">
   <children>
      <AnchorPane layoutX="100.0" layoutY="45.0">
         <children>
            <HBox>
               <children>
                   <Label text="请上传文件：" />
                  <TextField fx:id="filePath" />
                   <Button fx:id="uploadFile" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#uploadFile" text="上传" textAlignment="CENTER" />
               </children>
            </HBox>
            <HBox layoutY="49.0">
               <children>
                   <Label text="请输入系数：" />
                  <TextField fx:id="proportion" />
               </children>
            </HBox>
            <Button fx:id="handleDetailsFile" layoutX="90.0" layoutY="99.0" mnemonicParsing="false" onAction="#handleDetailsFile" text="计算" />
            <Button fx:id="clearButton" layoutX="174.0" layoutY="99.0" mnemonicParsing="false" onAction="#clearClick" text="清空" />
         </children>
      </AnchorPane>
      <AnchorPane layoutX="86.0" layoutY="196.0">
         <children>
            <TextArea fx:id="consoleInfo" prefHeight="179.0" prefWidth="500.0" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
